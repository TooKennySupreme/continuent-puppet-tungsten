{
  "name": "continuent/tungsten",
  "version": "1.0.5",
  "author": "Continuent",
  "summary": "Module for installing Continuent Tungsten",
  "license": "Apache-2.0",
  "source": "https://github.com/continuent/continuent-tungsten",
  "project_page": "https://github.com/continuent/continuent-tungsten",
  "issues_url": "https://github.com/continuent/continuent-tungsten/issues",
  "description": "Configure servers for running Continuent Tungsten",
  "dependencies": [
    {"name":"puppetlabs/stdlib","version_requirement":">= 2.2.1"},
    {"name":"puppetlabs/ntp","version_requirement":">= 3.0.0"},
    {"name":"puppetlabs/java","version_requirement":">= 1.0.1"},
    {"name":"puppetlabs/firewall","version_requirement":">= 0.4.0"},
    {"name":"puppetlabs/apt","version_requirement":">= 2.0.0"},
    {"name":"continuent/percona_repo","version_requirement":">= 0.1.0"},
    {"name":"continuent-selinux"},
    {"name":"puppetlabs/mysql","version_requirement":">= 3.1.0"},
    {"name":"fiddyspence/sysctl","version_requirement":">= 0.3.1"}
  ],
  "operatingsystem_support": [
    {
    "operatingsystem":"RedHat",
    "operatingsystemrelease":[ "5.0", "6.0","7.0" ]
    },
    {
    "operatingsystem": "Ubuntu",
    "operatingsystemrelease": [ "12.04", "10.04","14.04" ]
    }
   ]
}
